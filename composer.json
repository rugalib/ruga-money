{
  "name": "rugalib/ruga-money",
  "description": "Money library for ruga framework",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2",
    "ext-intl": "*",
    "ext-bcmath": "*",
    "rugalib/ruga-i18n": "^1.0",
    "rugalib/ruga-std": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "laminas/laminas-config-aggregator": "^1",
    "laminas/laminas-servicemanager": "^3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Money\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Money\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Money",
      "config-provider": "Ruga\\Money\\ConfigProvider"
    }
  }
}